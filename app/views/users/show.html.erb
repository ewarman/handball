<% provide(:title, @user.last_name) %>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1> <%= @user.first_name %> <%= @user.last_name %> </h1>
    <h2> Phone: <%= @user.phone %> </h2>
    <h2> Email:  <%= @user.email %> </h2>
    <h2> Notes: </h2>
    <h3> <%= @user.notes %> </h3>
    <%= link_to "Edit", edit_user_path(@user.id) %>
  </div>
</div>
